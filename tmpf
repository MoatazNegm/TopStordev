#! /usr/local/bin/zsh
web="/usr/local/www/apache24/data/des19/Data/poolstatus.txt";
pstatus=` /sbin/zpool list | grep Data `;
isok=` echo $pstatus | wc -c | awk '{print $1}'`;
if [[ $isok -ge 20 ]]; then
 raid='raidz2';
 ismirr=`zpool status Data | grep mirror | wc -c`;
 if [[ $ismirr -ge 5 ]]; then
	raid='mirror';
 fi
 israidz=`zpool status Data |grep raidz1 | wc -c`;
 if [[ $israidz -ge 5 ]]; then
 	raid='raidz';
 fi
 jsonok=`/usr/local/bin/zsh jsonthis3.sh status ok raid $raid`;
else
 jsonok=`/usr/local/bin/zsh jsonthis3.sh status no`;
fi
echo $jsonok > $web
#! /usr/local/bin/zsh
web="/usr/local/www/apache24/data/des19/Data/status.log";
zpool destroy Data 2>txt/err.txt
err=`wc -c txt/err.txt | awk '{print $1}'`;
if [[ err -ge 3 ]]; then
	cat txt/err.txt > $web;
else
	echo pool Data is removed with all containing data > $web;
fi 
#! /usr/local/bin/zsh
web="/usr/local/www/apache24/data/des19/Data/status.log";
raid=` echo $@ | awk '{print $2}'`;
result=` echo $@ | awk '{print $1}'`;
echo "creating Pool $1...." > $web
if [[ $raid == "mirror" ]]; then
 zpool create Data mirror da0 da1 mirror da2 da3 2>txt/err.txt 	
else
	zpool create Data $raid da0 da1 da2 da3  2>txt/err.txt
fi
err=`wc -c txt/err.txt | awk '{print $1}'`;
if [[ err -ge 3 ]]; then
	cat txt/err.txt > $web;
else
	echo pool Data of $result is created. > $web;
fi 
#! /usr/local/bin/zsh
web="/usr/local/www/apache24/data/des19";
loc=`echo $@ | awk '{print $1}'`;
disks=(`/sbin/sysctl kern.disks | sed -e 's/cd0 //g' -e 's/ada0//g' -e 's/kern.disks: //g'`);
noofdisks=` echo $disks | wc -w | awk '{print $1}'`;
onedisk=` cat onedisk.txt`;
xonedisk=`diskinfo -v $onedisk | sed -n -e 3p | awk '{print $1}'`;
sizeonedisk=$(($xonedisk/1024/1024/1024));
size=0;
for disk in  $disks
do
x=`diskinfo -v $disk | sed -n -e 3p | awk '{print $1}'`;
y=$(($x/1024/1024/1024));
size=$(($size+$y));
done
jsonsize=`/usr/local/bin/zsh jsonthis3.sh size $size count $noofdisks onedisk $sizeonedisk R10 97.9 R5 147 R6 97.7 R7 49`;
echo $jsonsize > $web/$loc;

#! /usr/local/bin/zsh
# needed the operands to be like : one:two:three:four
size=0;
for disk in "$@"
do
x=`diskinfo -v $disk | sed -n -e 3p | awk '{print $1}'`;
y=$(($x/1024/1024/1024));
size=$(($size+$y));
done
echo $size
#! /usr/local/bin/zsh
web="/usr/local/www/apache24/data/des19";
if [[ $1 < 2 ]]; then
	disks=('ada1');
else
	disks=(`/sbin/sysctl kern.disks | sed -e 's/cd0 //g' -e  's/ada0//g' -e 's/kern.disks: //g'`);
fi
size=0;
for disk in  $disks
do
x=`diskinfo -v $disk | sed -n -e 3p | awk '{print $1}'`;
y=$(($x/1024/1024/1024));
size=$(($size+$y));
done
jsonsize=`./jsonthis.sh $size`
echo $jsonsize > $web/$2;

#! /usr/local/bin/zsh
web="/usr/local/www/apache24/data/des19";
echo "Hard Coded from Factory"
#! /bin/sh
web='/usr/local/www/apache24/data/des19/Data/status.log';
rm txt/err; 
domain=`echo $@ | awk '{ print $1 }' `;
domc=`echo $domain | tr '[:lower:]' '[:upper:]'`;
doms=`echo $domain | tr '[:upper:]' '[:lower:]'`;
domshort=` echo $domc | awk -F. '{ print $1 }'`;
admin=`echo $@ | awk '{ print $2 }'`;
pass=`echo $@ | awk '{ print $3 }'`;
domcont=` echo $@ | awk '{ print $4 }'`
domcontip=` host $domcont.$doms | awk '{print $NF}'`
ip1=` echo $domcontip | awk -F. '{print $1}'`;
ip2=` echo $domcontip | awk -F. '{print $2}'`;
ip3=` echo $domcontip | awk -F. '{print $3}'`;
ip4=` echo $domcontip | awk -F. '{print $4}'`;
echo changing Domain to $domain.... > $web;
echo $pass > txt/pass;
hostn=`hostname | awk -F. '{print $1}'`;
sed "1s/.*/hostname=\"$hostn\.$doms\"/" /etc/rc.conf > txt/rc.conf;
hostname $hostn.$doms;
cp txt/rc.conf /etc/rc.conf;
/usr/bin/sed -e "s/DOMSMALL/${doms}/g" -e "s/DOMIPCONT/$ip1\.$ip2\.$ip3\.$ip4/g" dhclient.conf > txt/dhclient.conf 2>txt/err.txt
cp txt/dhclient.conf /etc/dhclient.conf
echo search_domains=\"$doms\" > txt/resolvconf.conf
#echo "domain $doms" >> txt/resolvconf.conf
echo name_servers=\"$domcontip\" >> txt/resolvconf.conf
#cat /etc/resolv.conf >> txt/resolv.conf;
cp txt/resolvconf.conf /etc/resolvconf.conf;
resolvconf -u;
sed -e "s/CONT/$domcont/g" -e "s/DOMSMALL/$doms/g" -e "s/DOMC/$domc/g" -e "s/DOMSH/$domshort/g" -e "s/DOMIPCONT/$ip1\.$ip2\.$ip3\.$ip4/g" smb4.conf > txt/smb4.conf 2>txt/err.txt
cp txt/smb4.conf /usr/local/etc/smb4.conf
sed -e "s/DOMC/$domc/g" -e "s/DOMSMALL/$doms/g" -e "s/DOMSH/$domshort/g" -e "s/CONT/$domcont/g" krb5.conf > txt/krb5.conf
cp txt/krb5.conf /etc/
sed -e "s/CONT/$domcont\.$doms/g" -e "s/DOMSMALL/$doms/g" ntp.conf > txt/ntp.conf
service samba_server restart;
cp txt/ntp.conf /etc/
service ntpd stop;
service ntpdate restart;
/usr/local/bin/net ads join  -U $admin%$pass
err=`wc -c  txt/err.txt | awk '{print $1}'`;
if [[ $err >  4  ]]; then
	cat txt/err.txt > $web;
else 
 echo DONE...Domain $doms is joined > $web; 
fi;
#! /usr/local/bin/zsh
cd /TopStor/;
websnaps='/usr/local/www/apache24/data/des19/Data/Hourlylist.txt';
list1=`crontab -l | awk -F"now" '{print $2}' | grep hourly`; 
list2=`echo $list1 | awk '{print $1"@"$2" "$2" "}'`;  
echo $list2
nameforjson=`echo $list2 | tr -d '\n'` ;
echo $nameforjson
jsonname=`./jsonthis2.sh $nameforjson`;
echo $jsonname > $websnaps 2>txt/err.txt

#! /usr/local/bin/zsh
cd /TopStor/;
websnaps='/usr/local/www/apache24/data/des19/Data/Minutelylist.txt';
list=`crontab -l | awk -F"now" '{print $2}'`;
echo $list;
result='[';
echo $list | while read -r line;
do
 father=`echo $line | awk '{print $1}' | awk -F/ '{print $2}' `;
 echo $father;
 ts=` echo $line | awk '{print $2}'`;
 period=`echo $ts | awk -F. '{print $1}' `;
 t1=` echo $ts  | awk -F. '{print $2}'`;	 
 t2=` echo $ts | awk -F. '{print $3}'`;
 t3=` echo $ts | awk -F. '{print $4}'`;
 t4=` echo $ts | awk -F. '{print $5}'`;
 subres=` /usr/local/bin/zsh jsonthis3.sh period $period father $father t1 $t1 t2 $t2 t3 $t3 t4 $t4`;
 result=$result$subres,
done
result=${result%?}]
echo $result
echo $result > $websnaps 2>txt/err.txt

#! /usr/local/bin/zsh
cd /TopStor/;
websnaps='/usr/local/www/apache24/data/des19/Data/Vollist.txt';
resweb='/usr/local/www/apache24/data/des19/Data/Vollist.txtupdated';
quota=`/sbin/zfs get -t filesystem -Hp quota | grep Data/  | awk -v CONVFMT=%2.5f '{gsub($3,$3/1048576); print $1" "$3}'`;
logicalused=`/sbin/zfs get -t filesystem -Hp logicalused | grep Data/ | awk -v CONVFMT=%.5g '{gsub($3,$3/1048576); print $1" "$3}'`
avail=`/sbin/zfs get -t filesystem -Hp avail | grep Data/ | awk -v CONVFMT=%.5g '{gsub($3,$3/1048576); print $1" "$3}'`;
used=`/sbin/zfs get -t filesystem -Hp used | grep Data/ | awk -v CONVFMT=%.5g '{gsub($3,$3/1048576); print $1" "$3}'` 
creation=`/sbin/zfs get -t filesystem -H creation | grep Data/  | awk  '{$3=$5"-"$4"-"$7; print $1" "$3}'`;
compressratio=`/sbin/zfs get -t filesystem -Hp compressratio | grep Data/ | awk  '{print $1" "$3}'`;
dedup=`/sbin/zfs get -t filesystem -Hp dedup | grep Data/ | awk '{print $1" "$3}'`;
CIFSshare=`/sbin/zfs get -t filesystem prot:kind  | grep Data/ | grep CIFS  | awk '{print $1" "$3}'`;
NFSshare=`/sbin/zfs get -t filesystem  prot:kind | grep Data/ | grep NFS | awk '{print $1" "$3}'`;
nameof=`/sbin/zfs get -t filesystem -Hp name | grep Data/ | awk -F/ '{print $2}' | awk '{print $1}'`;
result="[";
echo $nameof | while read -r line; do
	q=` echo $quota | grep "$line" | awk '{print $2}'`;
	lo=`echo $logicalused | grep "$line" | awk '{print $2}'`;
	av=`echo $avail | grep "$line" | awk '{print $2}'`;
	us=`echo $used | grep "$line" | awk '{print $2}'`;
	cr=`echo $creation | grep "$line" | awk '{print $2}'`;
	co=`echo $compressratio | grep "$line" | awk '{print $2}'`;
	de=`echo $dedup | grep "$line" | awk '{print $2}'`;
	nf=`echo $NFSshare | grep "$line" | awk '{print $2}'`;
	ci=`echo $CIFSshare | grep "$line" | awk '{print $2}'`;
	subres=`./jsonthis3.sh Pool Data name $line volsize $q volact $lo snaps $av used $us crdate $cr compress $co dedup $de protocol $ci$nf` ;
	result=$result$subres,
done;
result=`echo $result | rev | cut -c 2- | rev`]
echo $result > $websnaps 2>txt/err.txt
tstamp=`date +%s`;
res=` ./jsonthis3.sh updated $tstamp `;
echo $res > $resweb;

#! /usr/local/bin/zsh
cd /TopStor/;
websnaps='/usr/local/www/apache24/data/des19/Data/Vollist.txtCIFS';
quota=`/sbin/zfs get -t filesystem -Hp quota | grep Data/  | awk -v CONVFMT=%2.5f '{gsub($3,$3/1048576); print $1" "$3}'`;
logicalused=`/sbin/zfs get -t filesystem -Hp logicalused | grep Data/ | awk -v CONVFMT=%.5g '{gsub($3,$3/1048576); print $1" "$3}'`
avail=`/sbin/zfs get -t filesystem -Hp avail | grep Data/ | awk -v CONVFMT=%.5g '{gsub($3,$3/1048576); print $1" "$3}'`;
used=`/sbin/zfs get -t filesystem -Hp used | grep Data/ | awk -v CONVFMT=%.5g '{gsub($3,$3/1048576); print $1" "$3}'` 
creation=`/sbin/zfs get -t filesystem -H creation | grep Data/  | awk  '{$3=$5"-"$4"-"$7; print $1" "$3}'`;
compressratio=`/sbin/zfs get -t filesystem -Hp compressratio | grep Data/ | awk  '{print $1" "$3}'`;
dedup=`/sbin/zfs get -t filesystem -Hp dedup | grep Data/ | awk '{print $1" "$3}'`;
NFSshare=`/sbin/zfs get -t filesystem -Hp sharenfs | grep Data/ | grep off | awk '{print $1}'`;
alllist="";
echo $NFSshare | while read -r line; do
	q=` echo $quota | grep $line | awk '{print $2}'`;
	lo=`echo $logicalused | grep $line | awk '{print $2}'`;
	av=`echo $avail | grep $line | awk '{print $2}'`;
	us=`echo $used | grep $line | awk '{print $2}'`;
	cr=`echo $creation | grep $line | awk '{print $2}'`;
	co=`echo $compressratio | grep $line | awk '{print $2}'`;
	de=`echo $dedup | grep $line | awk '{print $2}'`;
	line2=`echo $line | awk -F/ '{print $2}'`;
	alllist=$alllist$line2" "$q"_"$us"_"$snaps"_"$lo"_"$cr"_"$av"_"$co"_"$de" ";
done;
nameforjson=`echo $alllist | tr -d '\n'` ;
jsonname=`./jsonthis2.sh $nameforjson`;
echo $jsonname > $websnaps 2>txt/err.txt

#! /usr/local/bin/zsh
cd /TopStor/;
websnaps='/usr/local/www/apache24/data/des19/Data/Vollist.txt';
quota=`/sbin/zfs get -Hp quota | grep Data/ | grep -v @ | awk -v CONVFMT=%.3g '{gsub($3,$3/1000000000); print $1" "$3}'`;
logicalused=`/sbin/zfs get -Hp logicalused | grep Data/ | grep -v @ | awk -v CONVFMT=%.3g '{gsub($3,$3/1000000000); print $1" "$3}'`
avail=`/sbin/zfs get -Hp avail | grep Data/ | grep -v @ | awk -v CONVFMT=%.3g '{gsub($3,$3/1000000000); print $1" "$3}'`;
used=`/sbin/zfs get -Hp used | grep Data/ | grep -v @ | awk -v CONVFMT=%.3g '{gsub($3,$3/1000000000); print $1" "$3}'` 
creation=`/sbin/zfs get -H creation | grep Data/ | grep -v @ | awk  '{$3=$5"-"$4"-"$7; print $1" "$3}'`;
compressratio=`/sbin/zfs get -Hp compressratio | grep Data/ | grep -v @ | awk  '{print $1" "$3}'`;
dedup=`/sbin/zfs get -Hp dedup | grep Data/ | grep -v @ | awk '{print $1" "$3}'`;
echo q=$quota 
echo lo=$logicalused
echo av=$avail
echo us=$used
echo cr=$creation
echo co=$compressratio
echo ded=$dedup
alllist="";
echo $avail | awk '{print $1}' | while read -r line; do
	snaps=`/sbin/zfs list -t snapshot | grep "$line" | wc -l | sed 's/ //g'`
	q=` echo $quota | grep $line | awk '{print $2}'`;
	lo=`echo $logicalused | grep $line | awk '{print $2}'`;
	av=`echo $avail | grep $line | awk '{print $2}'`;
	us=`echo $used | grep $line | awk '{print $2}'`;
	cr=`echo $creation | grep $line | awk '{print $2}'`;
	co=`echo $compressratio | grep $line | awk '{print $2}'`;
	de=`echo $dedup | grep $line | awk '{print $2}'`;
	line2=`echo $line | awk -F/ '{print $2}'`;
	alllist=$alllist$line2" "$q"_"$us"_"$snaps"_"$lo"_"$cr"_"$av"_"$co"_"$de" ";
done;
echo alllist=$alllist
nameforjson=`echo $alllist | tr -d '\n'` ;
echo $nameforjson; 
jsonname=`./jsonthis2.sh $nameforjson`;
echo $jsonname > $websnaps 2>txt/err.txt

