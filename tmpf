err=`wc -c  txt/err.txt | awk '{print $1}'`;
if [[ $err >  4  ]]; then
	cat txt/err.txt > $web;
else 
 echo DONE...user $hostn is added > $web; 
fi;
#!/bin/sh

. /etc/rc.subr

name="QuickStor"
start_cmd="${name}_start"
stop_cmd="${name}_stop"
rcvar=TopStor_enable
command="/TopStor/Quickstor.sh"
command_interpreter="/usr/local/bin/zsh"
load_rc_config $name
: ${QuickStor_enable:=no}
: ${TopStor_msg="QuickStor started successfully."}
QuickStor_start()
{
	echo "$TopStor_msg"
	/TopStor/Quickstor.sh &
}
QuickStor_stop()
{
	echo " it is stopped thanking God"
	pkill -HUP -f Quickstor
	touch /TopStor/txt/flag
}

run_rc_command "$1"
#!/bin/sh

. /etc/rc.subr

name="QuickStor"
start_cmd="${name}_start"
stop_cmd="${name}_stop"
rcvar=TopStor_enable
command="/TopStor/Quickstor.sh"
command_interpreter="/usr/local/bin/zsh"
load_rc_config $name
: ${QuickStor_enable:=no}
: ${TopStor_msg="QuickStor started successfully."}
QuickStor_start()
{
	echo "$TopStor_msg"
	/TopStor/Quickstor.sh &
}
QuickStor_stop()
{
	echo " it is stopped thanking God"
	pkill -HUP -f Quickstor
	touch /TopStor/txt/flag
}

run_rc_command "$1"
#!/usr/local/bin/zsh
ClearExit() {
	echo got a signal > /TopStor/txt/sigstatus.txt
	exit 0;
}
trap ClearExit HUP
while true; do 
{
 sleep 2
 if [[ `service TopStor status | awk '{print $3}'` = not ]]; then
  service TopStor start
 fi
 if [[ `service QuickStor2 status | awk '{print $3}'` = not ]]; then
  service QuickStor2 start
 fi
 sleep 2
}
done;
#!/usr/local/bin/zsh
ClearExit() {
	echo got a signal > /TopStor/txt/sigstatus.txt
	exit 0;
}
trap ClearExit HUP
while true; do 
{
 sleep 2 
 if [[ `service QuickStor status | awk '{print $3}'` = not ]]; then
  service QuickStor start
 fi
 sleep 2
}
done;
#! /usr/local/bin/zsh
openssl req -x509 -nodes -days 365 -newkey rsa:2048 -keyout topstor.key -out topstor.crt
#! /usr/local/bin/zsh
cd /TopStor
web='/usr/local/www/apache24/data/des19/Data/status.log';
echo Deleteing Snapshot $@.... > $web;
/sbin/zfs destroy $@  2>txt/err.txt;
err=`wc -c  txt/err.txt | awk '{print $1}'`;
if [[ $err >  4  ]]; then
	cat txt/err.txt > $web;
else 
 echo DONE...Snapshot $@ is deleted > $web; 
fi;
./GetSnaplist
