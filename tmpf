/sbin/zfs set prot:kind=CIFS $DG/$name
sed -e "s/NAMECIFS/$name/g" sharecifs.txt > $name.txt;
cat $name.txt >> smb4.conf;
cp smb4.conf /usr/local/etc/
service samba_server reload;
