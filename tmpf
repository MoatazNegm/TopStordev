hostdns=` cat /etc/resolv.conf | grep nameserver | awk '{print $2}'`;
jsonips=`/usr/local/bin/zsh jsonthis3.sh name $hostn addr $hostip rout $hostrout dns $hostdns`;
echo $jsonips > $web; 
