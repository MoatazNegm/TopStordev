/sbin/zfs create -o sharenfs=Yes -o quota=$quota -o dedup=on -o compression=on  $DG/$name 2> txt/err.txt 
err=`wc -c  txt/err.txt | awk '{print $1}'`;
if [[ $err >  4  ]]; then
	cat txt/err.txt > $web;
else 
 echo DONE...Volume $DG/$name is created > $web; 
fi;
./GetPoolVollist

