#! /usr/local/bin/zsh
cd /TopStor/;
websnaps='/usr/local/www/apache24/data/des19/Data/listsnaps.txt';
resweb='/usr/local/www/apache24/data/des19/Data/listsnaps.txtupdated';
namearray=`/sbin/zfs list -t snapshot -H -o name,creation`; 
result='[';
echo "$namearray" | while read -r line 
do
 name=` echo $line | awk '{print $1}'`;
 onlyname=` echo $name | awk -F@ '{print $2}'`;
 father2=` echo $name | awk -F@ '{print $1}' | awk -F/ '{print $2}'`;
 creat=` echo $line | awk '{print $2" "$3" "$4"  "$5" "$6}'`; 
 creall=`/bin/date -f %c $creat +%m/%d/%y_%R`;
 creatform=` echo $creall | awk -F_ '{print $1}'`;
 time2=` echo $creall | awk -F_ '{print $2}'`;
 subres=`/usr/local/bin/zsh jsonthis3.sh name $name onlyname $onlyname father $father2 creation $creatform time $time2; `;
 result=$result$subres,;
done
 result=`echo $result | rev | cut -c 2- | rev`];
echo $result >$websnaps;
resdate=`date +%s`;
res=` ./jsonthis3.sh updated $resdate`;
echo $res > $resweb;
#! /usr/local/bin/zsh
cd /TopStor/;
websnaps='/usr/local/www/apache24/data/des19/Data/listsnaps.txt';
namearray=`/sbin/zfs list -t snapshot -H -o name,creation | grep Data | awk -F/ '{print $2}' |  awk '{$NF="";$1=$1"_"; print $0}' | sed 's/ /_/g' | sed 's/:/;/g'`; 
echo $namearray
nameforjson=`echo $namearray | awk '{$NF=$NF" ";print $0}' | tr -d '\n' | sed 's/@/ /g' ` ;
echo $nameforjson
jsonname=`./jsonthis2.sh $nameforjson`;
echo $jsonname;
jsonmod=` echo $jsonname | sed 's/__/ @ /g' | sed 's/_/ /g' | sed 's/;/:/g'`
echo $jsonmod;
echo $jsonmod >$websnaps ;
#! /bin/sh
web='/usr/local/www/apache24/data/des19/Data/status.log';
rm txt/err; 
hostn=`echo $@ | awk '{ print $1 }' `;
hostip=`echo $@ | awk '{ print $2 }'`;
hostrout=`echo $@ | awk '{ print $3 }'`;
hostdns=` echo $@ | awk '{ print $4 }'`
echo changing box name and ips.... > $web;
echo name_servers=\"$hostdns\" > txt/resolvconf.conf
sed -e "s/HOSTNAME/$hostn/g" -e "s/IPADD/$hostip/g" -e "s/ROUTE/$hostrout/g" rc.conf > txt/rc.conf
cp txt/resolvconf.conf /etc/
cp txt/rc.conf /etc/
hostname $hostn
resolvconf -u
/etc/rc.d/netif restart && /etc/rc.d/routing restart;
route delete default;
route add default $hostrout
echo changes are done.. some moments and will see them > $web
